   "source": [
    "x = int(input())\n",
    "y = [int(y) for y in input().split(' ',x-1)]\n",
    "ans = y[0]\n",
    "an = y[0]\n",
    "for i in range(1, len(y)):\n",
    "        # ans = ans&y[i]\n",
    "        an |= y[i]\n",
    "print(an)"
